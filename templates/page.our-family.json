        "de5c2018-3e91-4b43-95a5-c41f51353606": {
          "type": "event",
          "settings": {
            "card_classes": "sm:-mt-40",
            "image_classes": "",
            "video_mp4": "shopify://files/videos/timeline-12.mp4",
            "video_classes": "w-full object-cover",
            "date": "2000",
            "paragraph": "<p class=\"mb-2\">We have been vegan and cruelty-free since the beginning. We affirmed this commitment by becoming Vegan certified by PETA.\nWe're also proud to be Cruelty Free-certified by Leaping Bunny since 2000.</p>"
          }
        },
        "c35522c1-924e-4779-a5d4-03f4c011f3fa": {
          "type": "event",
          "settings": {
            "card_classes": "sm:-mt-40",
            "image": "shopify://shop_images/maui-wailea.jpg",
            "image_classes": "",
            "video_classes": "",
            "date": "2002",
            "paragraph": "Our first spa partnership is with the Four Seasons Maui, uniting our shared passion for holistic wellness, naturally-derived ingredients and rejuvenation."
