      "blocks": {
        "a517ef73-142d-410f-b225-b26148b7070a": {
          "type": "heading",
          "settings": {
            "heading_text": "Generation of Women Inspired by the Sea <br>",
            "heading_classes": "font-serif text-center text-3xl tracking-tight mb-3",
            "subheading_text": "",
            "subheading_classes": "text-center mb-6 font-book tracking-wide sm:text-lg"
