        },
        "999dd57a-40d3-4863-8fbe-5e93b588aa8b": {
          "type": "event",
          "settings": {
            "card_classes": "sm:-mt-60",
            "image": "shopify://shop_images/timeline-435px604px-8_350x_813f4100-3702-4c91-9727-ba9a174ce08f.jpg",
            "image_classes": "",
            "video_classes": "",
            "date": "2004",
            "paragraph": "Jenefer testifies before the California State Assembly with the Environmental Working Group in support of the Safe Cosmetics Act, which passed in 2005. I also work with the Environmental Working Group and the Breast Cancer Fund to advocate for safer cosmetic regulation."
          }
        },
        "548df911-8b75-4a3b-88f1-cbc8481d2c2c": {
          "type": "event",
          "settings": {
