@font-face {
  font-family: 'Canela';
  src: url("{{ 'font-canela-light.woff2' | asset_url }}") format("woff2"), url("{{ 'font-canela-light.woff' | asset_url }}") format("woff");
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Canela';
  src: url("{{ 'font-canela-light-italic.woff2' | asset_url }}") format("woff2"), url("{{ 'font-canela-light-italic.woff' | asset_url }}") format("woff");
  font-weight: 400;
  font-style: italic;
  font-display: swap;
}

@font-face {
  font-family: "Circular";
  src: url("{{ 'CircularLLVarWeb-[wght].woff2' | asset_url }}") format("woff2");
  font-weight: 100 900;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: "Circular";
  src: url("{{ 'CircularLLVarWeb-Italic[wght].woff2' | asset_url }}") format("woff2");
  font-weight: 100 900;
  font-style: italic;
  font-display: swap;
}
