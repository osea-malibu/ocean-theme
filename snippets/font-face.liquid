@font-face {
  font-family: 'Canela';
  src: url("{{ 'font-canela-light.woff2' | asset_url }}") format("woff2");
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Canela';
  src: url("{{ 'font-canela-light-italic.woff2' | asset_url }}") format("woff2");
  font-weight: 400;
  font-style: italic;
  font-display: swap;
}

@font-face {
  font-family: "Circular";
  src: url("{{ 'font-circular-variable.woff2' | asset_url }}") format("woff2");
  font-weight: 100 900;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: "Circular";
  src: url("{{ 'font-circular-italic-variable.woff2' | asset_url }}") format("woff2");
  font-weight: 100 900;
  font-style: italic;
  font-display: swap;
}
