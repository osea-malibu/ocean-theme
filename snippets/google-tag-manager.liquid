<script>
    const urlParams = new URLSearchParams(window.location.search);
    const utmSource = urlParams.get('utm_source');
    const utmMedium = urlParams.get('utm_medium');
    const utmCampaign = urlParams.get('utm_campaign');
    if (utmSource || utmMedium || utmCampaign) {
      let channel = "";
      if (utmSource === "tiktok_ad") channel = "Purchase_TikTok"
      else if (utmSource === "pinterest-ad") channel = "Purchase_Pinterest"
      else if (utmSource === "tiktok") channel = "Purchase_Organic-TikTok"
      else if (utmSource === "likeshop") channel = "Purchase_Organic-IG"
      else if (utmSource === "refersion") channel = "Purchase_Affiliate-Refersion"
      else if (utmSource === "shareasale") channel = "Purchase_Affiliate-Shareasale"
      else if (utmMedium === "email") channel = "Purchase_Email"
      else if (utmMedium === "sms") channel = "Purchase_SMS"
      else if (utmSource === "google") {
        if (utmCampaign.includes("nonbrand")) channel = "Purchase_Google-NonBranded"
        else if (utmCampaign.includes("brand")) channel = "Purchase_Google-Branded"
        else if (utmCampaign.includes("youtube")) channel = "Purchase_Google-Youtube"
        else if (utmCampaign.includes("Pmax")) channel = "Purchase_Google-Shopping"
        else if (utmCampaign.includes("Display") || utmCampaign.includes("Discovery")) channel = "Purchase_Google-Display"
      };
      else if (channel == "") {channel = "Other"}
      localStorage.setItem('utm_source', channel);
    }
</script>